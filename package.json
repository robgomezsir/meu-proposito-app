{
  "name": "sistema-proposito",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "clsx": "^2.1.1",
    "firebase": "^9.23.0",
    "lucide-react": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-render": "node scripts/deploy-config.js && react-scripts build",
    "build-gh": "cross-env REACT_APP_ENV=github-pages node scripts/deploy-config.js && react-scripts build",
    "build-local": "cross-env REACT_APP_ENV=local node scripts/deploy-config.js && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build-gh",
    "deploy": "echo 'Deploy via GitHub Actions - execute: git push origin main'",
    "deploy-gh": "gh-pages -d build --repo https://github.com/robgomezsir/meu-proposito-app.git"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://robgomezsir.github.io/meu-proposito-app"
}